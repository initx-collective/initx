{
  "name": "@initx/monorepo",
  "type": "module",
  "version": "0.0.28",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "description": "A more convenient scripting engine",
  "license": "MIT",
  "homepage": "https://github.com/initx-collective/initx#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:initx-collective/initx.git"
  },
  "bugs": {
    "url": "https://github.com/initx-collective/initx/issues"
  },
  "scripts": {
    "stub": "pnpm -r --filter=./packages/** run stub",
    "build": "pnpm -r --filter=./packages/** run build",
    "test": "vitest",
    "tu": "vitest -u",
    "lint": "eslint --cache",
    "release": "bumpp -r",
    "initx": "node ./packages/cli/bin/initx.mjs",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@imba97/eslint-config": "^0.0.5",
    "@types/node": "^22.10.7",
    "bumpp": "^9.10.1",
    "defu": "^6.1.4",
    "eslint": "^9.18.0",
    "lint-staged": "^15.4.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --cache --fix"
  }
}
